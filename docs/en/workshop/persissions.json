{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "iam:CreatePolicy",
                "iam:GetRole",
                "iam:PassRole",
                "iam:DetachRolePolicy",
                "iam:DeleteRolePolicy",
                "iam:DeletePolicy",
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:AttachRolePolicy",
                "iam:UpdateRole",
                "iam:PutRolePolicy",
                "iam:TagRole",
                "iam:TagPolicy",
                "iam:CreatePolicyVersion",
                "iam:GetRolePolicy",
                "iam:GetPolicy",
                "iam:GetPolicyVersion",
                "iam:ListInstanceProfilesForRole"
            ],
            "Resource": [
                "arn:aws:iam::*:policy/qcedd*",
                "arn:aws:iam::*:policy/QCEDD*",
                "arn:aws:iam::*:role/qcedd*",
                "arn:aws:iam::*:role/QCEDD*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutEncryptionConfiguration",
                "s3:PutBucketAcl",
                "s3:PutBucketPolicy",
                "s3:ListBucket",
                "s3:DeleteBucketPolicy",
                "s3:DeleteBucket",
                "s3:PutBucketTagging",
                "s3:CreateBucket",
                "s3:PutBucketLogging",
                "s3:GetBucketLocation",
                "s3:ListBucketMultipartUploads",
                "s3:ListMultipartUploadParts",
                "s3:AbortMultipartUpload",
                "s3:PutBucketPublicAccessBlock"
            ],
            "Resource": [
                "arn:aws:s3:::qcedd*",
                "arn:aws:s3:::braket*",
                "arn:aws:s3:::amazon-braket*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:PassRole"
            ],
            "Resource": [
                "arn:aws:iam::*:role/ecsInstanceRole",
                "arn:aws:iam::*:instance-profile/ecsInstanceRole",
                "arn:aws:iam::*:role/iaws-ec2-spot-fleet-role",
                "arn:aws:iam::*:role/aws-ec2-spot-fleet-role"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::qcedd*/*",
                "arn:aws:s3:::braket*/*",
                "arn:aws:s3:::amazon-braket*/*",
                "arn:aws:s3:::aws-gcr-solutions*/*",
                "arn:aws:s3:::solutions*/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "ecr:PutImage",
                "ecr:InitiateLayerUpload",
                "ecr:CompleteLayerUpload",
                "ecr:CreateRepository",
                "ecr:DeleteRepository"
            ],
            "Resource": [
                "arn:aws:ecr:*:*:repository/qc*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "sagemaker:CreatePresignedNotebookInstanceUrl",
                "sagemaker:Describe*",
                "sagemaker:StopNotebookInstance",
                "sagemaker:StartNotebookInstance",
                "sagemaker:DeleteNotebookInstance*",
                "sagemaker:CreateNotebookInstance*"
            ],
            "Resource": [
                "arn:aws:sagemaker:*:*:notebook-instance/notebook*",
                "arn:aws:sagemaker:*:*:notebook-instance-lifecycle-config/notebook*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "sns:Publish",
                "sns:DeleteTopic",
                "sns:CreateTopic",
                "sns:SetTopicAttributes",
                "sns:Subscribe",
                "sns:ConfirmSubscription"
            ],
            "Resource": [
                "arn:aws:sns:*:*:QCEDD*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "cloudformation:CreateStack",
                "cloudformation:GetTemplate",
                "cloudformation:DeleteStack",
                "cloudformation:UpdateStack",
                "cloudformation:ExecuteChangeSet",
                "cloudformation:CreateChangeSet",
                "cloudformation:DeleteChangeSet",
                "cloudformation:CreateUploadBucket"
            ],
            "Resource": [
                "arn:aws:cloudformation:*:*:stack/QCEDD*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "events:ListRules",
                "events:ListEventBuses",
                "events:DeleteRule",
                "events:PutTargets",
                "events:RemoveTargets",
                "events:PutRule",
                "athena:Get*",
                "athena:List*",
                "athena:BatchGet*",
                "athena:RunQuery",
                "glue:CreateDatabase",
                "glue:DeleteDatabase",
                "glue:Get*",
                "glue:CreateTable",
                "glue:DeleteTable",
                "events:Describe*",
                "cloudformation:GetTemplateSummary",
                "cloudformation:ListStacks",
                "cloudformation:ValidateTemplate",
                "cloudformation:Describe*",
                "cloudformation:List*",
                "kms:DescribeKey",
                "kms:ListKeys",
                "kms:ListAliases",
                "kms:CreateKey",
                "kms:PutKeyPolicy",
                "kms:EnableKeyRotation",
                "kms:CreateGrant",
                "ssm:GetParameter",
                "sts:AssumeRole",
                "states:*",
                "batch:*",
                "quicksight:List*",
                "quicksight:Describe*",
                "quicksight:Get*",
                "quicksight:*DataSet",
                "quicksight:*Analysis",
                "quicksight:*DataSource",
                "quicksight:*Template",
                "quicksight:*Dashboard*",
                "quicksight:UpdateResourcePermissions",
                "quicksight:RegisterUser",
                "quicksight:CreateAdmin",
                "sagemaker:DescribeNotebookInstance*",
                "sagemaker:ListNotebookInstances",
                "sagemaker:ListCodeRepositories",
                "lambda:List*",
                "lambda:Get*",
                "lambda:InvokeFunction",
                "lambda:CreateFunction",
                "lambda:CreateFunction",
                "lambda:UpdateFunctionCode",
                "lambda:PutFunctionEventInvokeConfig",
                "lambda:DeleteFunction",
                "lambda:PutFunctionConcurrency",
                "lambda:AddPermission",
                "lambda:RemovePermission",
                "ecs:Describe*",
                "ecr:List*",
                "ecr:Get*",
                "ecr:BatchGetImage",
                "ecr:GetAuthorizationToken",
                "braket:*",
                "logs:Describe*",
                "logs:Get*",
                "logs:DeleteLogGroup",
                "logs:CreateLogGroup",
                "logs:PutRetentionPolicy",
                "logs:DeleteRetentionPolicy",
                "cloudwatch:GetMetricStatistics",
                "ec2:Get*",
                "ec2:Describe*",
                "ec2:AcceptVpcEndpointConnections",
                "ec2:AllocateAddress",
                "ec2:AssignPrivateIpAddresses",
                "ec2:Associate*",
                "ec2:Attach*",
                "ec2:AuthorizeSecurityGroup*",
                "ec2:CreateDhcpOptions",
                "ec2:CreateEgress*",
                "ec2:CreateFlowLogs",
                "ec2:CreateInternetGateway",
                "ec2:CreateNatGateway",
                "ec2:CreateNetwork*",
                "ec2:CreateRoute*",
                "ec2:CreateSecurityGroup",
                "ec2:CreateSubnet",
                "ec2:CreateTags",
                "ec2:CreateVpc*",
                "ec2:DeleteEgress*",
                "ec2:DeleteFlowLogs",
                "ec2:DeleteInternetGateway",
                "ec2:DeleteNatGateway",
                "ec2:DeleteNetwork*",
                "ec2:DeleteRoute*",
                "ec2:DeleteSecurityGroup",
                "ec2:DeleteSubnet",
                "ec2:DeleteTags",
                "ec2:DeleteVpc*",
                "ec2:Detach*",
                "ec2:Disassociate*",
                "ec2:ModifyNetworkInterfaceAttribute",
                "ec2:ModifySecurityGroupRules",
                "ec2:ModifySubnetAttribute",
                "ec2:ModifyVpc*",
                "ec2:MoveAddressToVpc",
                "ec2:ReleaseAddress",
                "ec2:RevokeSecurityGroup*",
                "s3:ListAllMyBuckets",
                "iam:ListPolicies",
                "iam:ListRoles",
                "iam:ListRolePolicies",
                "iam:ListAttachedRolePolicies",
                "iam:CreateInstanceProfile",
                "iam:DeleteInstanceProfile",
                "iam:RemoveRoleFromInstanceProfile",
                "iam:AddRoleToInstanceProfile",
                "iam:GetInstanceProfile",
                "sns:List*",
                "sns:Get*"
            ],
            "Resource": "*"
        }
    ]
}